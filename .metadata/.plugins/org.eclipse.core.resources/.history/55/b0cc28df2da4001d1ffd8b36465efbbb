
public class Main {

	public static void main(String[] args) {
		System.out.println("Hello World!");
		System.out.println("Welcome to Hue City");
		int n = 19;
		Main m = new Main();
		boolean rs = m.LaChan(n);
		if (rs)
			System.out.print(n + " la so chan");
		else
			System.out.printf("%d la so le", n);
	}
	boolean LaChan(int n) {
		if (n % 2 == 0)
			return true;
		return false;
	}
}
